{
  "name": "Payment Schedule Entry",
  "module": "UW Core",
  "doctype": "DocType",
  "engine": "InnoDB",
  "istable": 1,
  "editable_grid": 1,
  "track_changes": 0,
  "fields": [
    {
      "fieldname": "due_date",
      "fieldtype": "Date",
      "label": "Due Date",
      "reqd": 1,
      "in_list_view": 1,
      "columns": 2
    },
    {
      "fieldname": "expected_amount",
      "fieldtype": "Currency",
      "label": "Expected Amount",
      "reqd": 1,
      "in_list_view": 1,
      "columns": 2
    },
    {
      "fieldname": "status",
      "fieldtype": "Select",
      "label": "Status",
      "options": "Pending\nPaid\nOverdue\nPartially Paid\nWritten Off",
      "default": "Pending",
      "in_list_view": 1,
      "columns": 1
    },
    {
      "fieldname": "actual_amount",
      "fieldtype": "Currency",
      "label": "Actual Amount",
      "in_list_view": 1,
      "columns": 2,
      "read_only": 1
    },
    {
      "fieldname": "donation",
      "fieldtype": "Link",
      "label": "Donation",
      "options": "Donation",
      "read_only": 1,
      "in_list_view": 1,
      "columns": 2,
      "description": "Matched donation payment"
    },
    {
      "fieldname": "notes",
      "fieldtype": "Small Text",
      "label": "Notes"
    }
  ]
}